type=PariWithWindow
ptype=RegExp
pattern=sshd\[\d+\]:, Failed .+ for (\S+) from ([\d.]+) port \d+ ssh2
desc=User $1 entered the wrong password
action=none
ptype2=RegExp
pattern2=sshd\[\d+\]:, Accepted .+ for $1 from $2 port \d+ ssh2
desc2=User $1 logged in from $2 after initial failure 
action2=write /var/log/collector/sec.log User $1 logged in from $2 after initial failure
window=300
